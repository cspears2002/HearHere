Upload(ist)
<%= form_for [@user, @sound], :url => "/users/:user_id/sounds", :html => { :multipart => true } do |form| %>
	<%= form.file_field :sound_file, class: "upload_sound_btn" %><br><br>
  <%= form.text_field :sound_name, placeholder: "Sound name", class: "new_sound_form" %><br>
  <%= form.text_field :description, placeholder: "Description", class: "new_sound_form" %><br>
  <%= form.text_field :location, placeholder: "Location", class: "new_sound_form" %><br>
  <div class="field">
    <%= form.text_field :tag_list, placeholder: "Tags (separated by commas)", class: "new_sound_form" %><br>
  </div>
  <%= form.submit "Add Sound", class: "new_sound_btn" %>
<% end %>
